{% extends 'baseAdmin.html.twig' %}

{% block title %}Pays index{% endblock %}

{% block body %}
    <h1>Pays index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for pay in pays %}
            <tr>
                <td>{{ pay.id }}</td>
                <td>{{ pay.nom }}</td>
                <td>
                    <a class="btn btn-success" href="{{ path('pays_show', {'id': pay.id}) }}">show</a>
                    <a class="btn btn-success" href="{{ path('pays_edit', {'id': pay.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a class="btn btn-success" href="{{ path('pays_new') }}">Create new</a>
{% endblock %}

